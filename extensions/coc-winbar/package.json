{
  "name": "coc-winbar",
  "version": "0.1.0",
  "description": "",
  "author": "ph <chenyi.ph@outlook.com>",
  "license": "MIT",
  "main": "out.js",
  "keywords": [
    "coc.nvim"
  ],
